scalar Date

schema {
    query: Query,
    mutation: Mutation,
}

type Mutation{
    tokenRefresh(request: TokenRefreshRequestDto): LoginResponseDto
    signup(request: KakaoLoginRequestDto): LoginResponseDto

    askGpt(question:String!):Question!

    requestPlanInformation(request: PlanRequestDto): String
}

type Query{
    userInfo: UserInfoDto
}

type Question{
    question:String
}